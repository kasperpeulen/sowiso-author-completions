{
  "manifest_version": 2,
  "name": "sowiso-author-completions",
  "version": "0.0.0",
  "description": "Provides completions for authors working in sowiso.",
  "content_scripts": [
    {
      "matches": [
        "*://*.sowiso.nl/*"
      ],
      "js": [
        "index.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "sowiso_php_functions.json",
    "standard_php_functions.json"
  ]
}
